{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true, // Enable support for JavaScript files
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.js", // Include all JavaScript files
    "**/*.ts",
    "**/*.tsx"
  ]
}
